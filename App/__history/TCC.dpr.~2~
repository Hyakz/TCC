program TCC;

uses
  Vcl.Forms,
  uBase in '..\Herança\uBase.pas' {frmBase},
  uCadBase in '..\Herança\uCadBase.pas' {frmCadBase},
  uPsqBase in '..\Herança\uPsqBase.pas' {frmBase1};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TfrmBase, frmBase);
  Application.CreateForm(TfrmCadBase, frmCadBase);
  Application.CreateForm(TfrmBase1, frmBase1);
  Application.Run;
end.
